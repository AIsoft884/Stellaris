ssb_locrora_system_initializer = {
	name = "Locrora"
	class = "sc_m"
	asteroid_belt = {
		type = rocky_asteroid_belt
		radius = 145
	}
	usage = custom_empire
	init_effect = {
		every_neighbor_system = {
			set_star_flag = empire_cluster
			every_neighbor_system = {
				set_star_flag = empire_cluster
			}
		}
		generate_home_system_resources = yes
	}

	planet = {
		name = "Locrora"
		class = "pc_m_star"
		orbit_distance = 0
		orbit_angle = 1
		size = 17
		has_ring = no
	}

	planet = {
		class = "pc_asteroid"
		orbit_distance = 60
		orbit_angle = { min = 90 max = 270 }
		size = 6
		has_ring = no
	}
	planet = {
		class = "pc_asteroid"
		orbit_distance = 0
		orbit_angle = { min = 90 max = 270 }
		size = 7
		has_ring = no
	}
	planet = {
		class = "pc_asteroid"
		orbit_distance = 0
		orbit_angle = { min = 90 max = 270 }
		size = 2
		has_ring = no
	}
	planet = {
		class = "pc_barren_cold"
		orbit_distance = 50
		orbit_angle = { min = 90 max = 270 }
		size = 12
		has_ring = no
		tile_blockers = none
		modifiers = none
		init_effect = {
			prevent_anomaly = yes
		}
	}
	planet = {
		class = "pc_frozen"
		orbit_distance = 35
		orbit_angle = { min = 90 max = 270 }
		size = 14
		has_ring = no
		tile_blockers = none
		modifiers = none
		init_effect = {
			prevent_anomaly = yes
		}
	}

}
