ssb_uapra_system_initializer = {
	name = "Uapra"
	class = "sc_m"
	usage = custom_empire
	init_effect = {
		every_neighbor_system = {
			set_star_flag = empire_cluster
			every_neighbor_system = {
				set_star_flag = empire_cluster
			}
		}
		generate_home_system_resources = yes
	}

	planet = {
		name = "Uapra"
		class = "pc_m_star"
		orbit_distance = 0
		orbit_angle = 1
		size = 11
		has_ring = no
	}

	planet = {
		class = "pc_gas_giant"
		orbit_distance = 75
		orbit_angle = { min = 90 max = 270 }
		size = 10
		change_orbit = 5
		has_ring = no
		tile_blockers = none
		modifiers = none
		init_effect = {
			prevent_anomaly = yes
		}
		moon = {
			class = "pc_asteroid"
			orbit_distance = 12
			orbit_angle = { min = 90 max = 270 }
			size = 4
			tile_blockers = none
			modifiers = none
		}
	}
	planet = {
		class = "pc_barren"
		orbit_distance = 35
		orbit_angle = { min = 90 max = 270 }
		size = 4
		has_ring = no
		tile_blockers = none
		modifiers = none
		init_effect = {
			prevent_anomaly = yes
		}
	}
	planet = {
		class = "pc_asteroid"
		orbit_distance = 35
		orbit_angle = { min = 90 max = 270 }
		size = 14
		has_ring = no
		tile_blockers = none
		modifiers = none
		init_effect = {
			prevent_anomaly = yes
		}
	}

}
