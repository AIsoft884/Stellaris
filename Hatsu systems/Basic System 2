ssb_yaclite_system_initializer = {
	name = "Yaclite"
	class = "sc_g"
	usage = custom_empire
	init_effect = {
		every_neighbor_system = {
			set_star_flag = empire_cluster
			every_neighbor_system = {
				set_star_flag = empire_cluster
			}
		}
		generate_home_system_resources = yes
	}

	planet = {
		name = "Yaclite"
		class = "pc_g_star"
		orbit_distance = 0
		orbit_angle = 1
		size = 26
		has_ring = no
	}

	planet = {
		class = "pc_ocean"
		orbit_distance = 75
		orbit_angle = { min = 90 max = 270 }
		size = 3
		has_ring = yes
		change_orbit = 5
		tile_blockers = none
		modifiers = none
		moon = {
			class = "pc_asteroid"
			orbit_distance = 12
			orbit_angle = { min = 90 max = 270 }
			size = 10
			tile_blockers = none
			modifiers = none
		}
	}
	planet = {
		class = "pc_gas_giant"
		orbit_distance = 35
		orbit_angle = { min = 90 max = 270 }
		size = 22
		change_orbit = 5
		has_ring = no
		tile_blockers = none
		modifiers = none
		init_effect = {
			prevent_anomaly = yes
		}
	}
	planet = {
		class = "pc_gas_giant"
		orbit_distance = 35
		orbit_angle = { min = 90 max = 270 }
		size = 14
		change_orbit = 5
		has_ring = no
		tile_blockers = none
		modifiers = none
		init_effect = {
			prevent_anomaly = yes
		}
	}
	planet = {
		class = "pc_ocean"
		orbit_distance = 35
		orbit_angle = { min = 90 max = 270 }
		size = 4
		has_ring = no
		tile_blockers = none
		modifiers = none
		init_effect = {
			prevent_anomaly = yes
		}
		moon = {
			class = "pc_frozen"
			orbit_distance = 12
			orbit_angle = { min = 90 max = 270 }
			size = 13
			tile_blockers = none
			modifiers = none
		}
	}
	planet = {
		class = "pc_asteroid"
		orbit_distance = 35
		orbit_angle = { min = 90 max = 270 }
		size = 15
		has_ring = no
		tile_blockers = none
		modifiers = none
	}

