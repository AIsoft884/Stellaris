ssb_lelrinov_system_initializer = {
	name = "Lelrinov"
	class = "sc_m"
	asteroid_belt = {
		type = rocky_asteroid_belt
		radius = 115
	}
	usage = custom_empire
	init_effect = {
		every_neighbor_system = {
			set_star_flag = empire_cluster
